<?xml version="1.0" encoding="utf-8" ?>
<module>
    <id>VirtoCommerce.WebHooksModule</id>
    <version>0.1.1</version>
    <platformVersion>2.13.52</platformVersion>
    <dependencies>
        <dependency id="VirtoCommerce.Core" version="2.25.29" />
    </dependencies>

    <title>Webhooks</title>
    <description>Manage user defined webhooks</description>
    <authors>
        <author>Eugeny Tatarincev</author>
        <author>Andrey Kozlov</author>
        <author>Aleksandr Andreev</author>
    </authors>
    <owners>
      <owner>Virto Commerce</owner>
    </owners>

    <releaseNotes></releaseNotes>
    <copyright>Copyright Â© 2019 . All rights reserved</copyright>
    <tags>webhooks module</tags>
  
    <assemblyFile>VirtoCommerce.WebHooksModule.Web.dll</assemblyFile>
    <moduleType>VirtoCommerce.WebHooksModule.Web.Module, VirtoCommerce.WebHooksModule.Web</moduleType>

    <styles>
        <directory virtualPath="$/Content/css" searchPattern="*.css" searchSubdirectories="true" />
    </styles>
    <scripts>
        <directory virtualPath="$/Scripts" searchPattern="*.js" searchSubdirectories="true" />
    </scripts>

    <permissions>
        <group name="Webhooks">
            <permission id="webhooks:access" name="Open webhooks menu" />
            <permission id="webhooks:read" name="Read webhooks" />
            <permission id="webhooks:feed:read" name="Read webhook logs" />
            <permission id="webhooks:execute" name="Manually send webhooks" />
            <permission id="webhooks:update" name="Create or update webhooks" />
            <permission id="webhooks:delete" name="Delete webhooks" />
        </group>
    </permissions>
</module>
